{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title_template": "{{ .Folder }}",
  "pwd": "osc99",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "leading_diamond": "\ue0b6",
          "template": "{{ .Icon }} ",
          "foreground": "#00c983",
          "background": "#003543",
          "type": "os",
          "style": "diamond"
        },
        {
          "leading_diamond": "\ue0b6",
          "template": "{{ .UserName }} ",
          "foreground": "#00c983",
          "background": "#003543",
          "type": "session",
          "style": "diamond"
        },
        {
          "properties": {
            "home_icon": "~",
            "style": "mixed",
	    "mapped_locations": {
	    }
          },
          "foreground": "black",
          "powerline_symbol": "\ue0b0",
          "background": "#DA627D",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "template": "{{ if .UpstreamURL }}{{ url .UpstreamIcon .UpstreamURL }}{{ end }} {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }}",
          "foreground": "black",
          "powerline_symbol": "\ue0b0",
          "background": "#FCA17D",
          "type": "git",
          "style": "powerline"
        },
        {
          "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#76b367",
          "type": "node",
          "style": "powerline"
        },
        {
          "properties": {
            "time_format": "3:04 PM"
          },
          "template": " \uf017 {{ .CurrentDate | date .Format }} ",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#33658A",
          "type": "time",
          "style": "powerline"
        },
        {
          "properties": {
            "always_enabled": true
          },
          "trailing_diamond": "\ue0b4",
          "template": " {{ if gt .Code 0 }}\uf421{{ else }}\uf469{{ end }} ",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#2e9599",
          "type": "status",
          "style": "diamond",
          "background_templates": [
            "{{ if gt .Code 0 }}red{{ end }}"
          ]
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "template": "\ue3bf ",
          "foreground": "#cd5e42",
          "type": "root",
          "style": "plain"
        },
        {
          "template": " <#26C6DA>❯</><#45F1C2>❯</>",
          "foreground": "#CD4277",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
